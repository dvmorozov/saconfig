@model IEnumerable<SAConfig.Models.saconfig_tSDI>

@{
    ViewBag.Title = "SDI";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SDI</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            desc
        </th>
        <th>
           name
        </th>
        <th>
            ix
        </th>
        <th>
            SDI (owner)
        </th>
        <th>
            DOI
        </th>
        <th>
            Owner Type
        </th>
        <th>
            DataOwnerID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tAttributeNameEnum.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ix)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tSDI2.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tDOI.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_SDIOwnerType.OwnerType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataOwnerID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
