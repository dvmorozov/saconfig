@model SAConfig.Models.saconfig_tServices

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<fieldset>
    <legend>Services Attributes</legend>

    <div class="display-label">nameLength</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.nameLength)
    </div>

    <div class="display-label">IED</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.saconfig_tIED.name)
    </div>

</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.ID, iedID = (long)ViewBag.IEDID, backURL = (string)(ViewBag.BackURL), ownerType = (string)(ViewBag.OwnerType) }) |
    <a href="@Html.Raw((String)(ViewBag.BackURL))">Back to Upper Element</a>
</p>
