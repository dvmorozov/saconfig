@model IEnumerable<SAConfig.Models.saconfig_tSubEquipment>

@{
    ViewBag.Title = "SubEquipment";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>SubEquipment</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            phase
        </th>
        <th>
            virtual
        </th>
        <th>
            TransformerWinding
        </th>
        <th>
            ConductingEquipment
        </th>
        <th>
            Owner Type
        </th>
        <th>
            DataOwnerID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tPhaseEnum.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.virtual_)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tTransformerWinding.type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tConductingEquipment.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_SubEquipmentOwnerType.OwnerType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataOwnerID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
