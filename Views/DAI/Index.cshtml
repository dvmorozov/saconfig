@model IEnumerable<SAConfig.Models.saconfig_tDAI>

@{
    ViewBag.Title = "DAI";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>DAI</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            desc
        </th>
        <th>
            name
        </th>
        <th>
            sAddr
        </th>
        <th>
            valKind
        </th>
        <th>
            ix
        </th>
        <th>
            DOI
        </th>
        <th>
            SDI
        </th>
        <th>
            Owner Type
        </th>
        <th>
            DataOwnerID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tAttributeNameEnum.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sAddr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tValKindEnum.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ix)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tDOI.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tSDI.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_DAIOwnerType.OwnerType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataOwnerID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
