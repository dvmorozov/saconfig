    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ConfLNs</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            fixPrefix
        </th>
        <th>
            fixLnInst
        </th>
        <th>
            Services
        </th>
        <th>
            DataOwnerID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
