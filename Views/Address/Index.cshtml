@model IEnumerable<SAConfig.Models.saconfig_tAddress>

@{
    ViewBag.Title = "Address";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Address</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            ConnectedAP
        </th>
        <th>
            SMV
        </th>
        <th>
            GSE
        </th>
        <th>
            Owner Type
        </th>
        <th>
            DataOwnerID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tConnectedAP.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tSMV.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tGSE.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_AddressOwnerType.OwnerType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataOwnerID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
