@model SAConfig.Models.saconfig_tCertificate

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>@Html.Raw((string)ViewBag.ElementName) Attributes</legend>

    @Html.Hidden("upperLevelID", (long)ViewBag.UpperLevelID)

    <div class="display-label">xferNumber</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.xferNumber)
    </div>

    <div class="display-label">serialNumber</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.serialNumber)
    </div>

    <div class="display-label">desc</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.desc)
    </div>

    <div class="display-label">name</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.name)
    </div>

    <div class="display-label">AccessPoint</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.saconfig_tAccessPoint.desc)
    </div>

    <div class="display-label">Element Name</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.saconfig_CertificateElementName.ElementName)
    </div>

</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        <a href="@Html.Raw((String)(ViewBag.BackURL))">Back to Upper Element</a>
    </p>
}
