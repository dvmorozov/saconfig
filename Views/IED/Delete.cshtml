        @Html.DisplayFor(model => model.saconfig_tRightEnum.value)
    </div>

    <div class="display-label">owner</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.owner)
    </div>

    <div class="display-label">SCL</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.saconfig_SCL.version)
    </div>

</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        <a href="@Html.Raw((String)(ViewBag.BackURL))">Back to Upper Element</a>
    </p>
}
