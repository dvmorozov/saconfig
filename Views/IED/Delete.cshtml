    <div class="display-label">configVersion</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.configVersion)
    </div>

    <div class="display-label">originalSclVersion</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.originalSclVersion)
