@model SAConfig.Models.saconfig_tLogSettings

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edit</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>LogSettings Attributes</legend>

        @Html.HiddenFor(model => model.ID)

        <div class="editor-label">
            @Html.LabelFor(model => model.logEna, "logEna")
        </div>
        <div class="editor-field">
            @Html.DropDownList("logEna", String.Empty)
            @Html.ValidationMessageFor(model => model.logEna)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.trgOps, "trgOps")
        </div>
        <div class="editor-field">
            @Html.DropDownList("trgOps", String.Empty)
            @Html.ValidationMessageFor(model => model.trgOps)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.intgPd, "intgPd")
        </div>
        <div class="editor-field">
            @Html.DropDownList("intgPd", String.Empty)
            @Html.ValidationMessageFor(model => model.intgPd)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Services, "Services")
        </div>
        <div class="editor-field">
            @Html.DropDownList("Services", String.Empty)
            @Html.ValidationMessageFor(model => model.Services)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
