@model IEnumerable<SAConfig.Models.saconfig_tConnectedAP>

@{
    ViewBag.Title = "ConnectedAP";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ConnectedAP</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            desc
        </th>
        <th>
            iedName
        </th>
        <th>
            apName
        </th>
        <th>
            SubNetwork
        </th>
        <th>
            DataOwnerID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.iedName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.apName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tSubNetwork.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataOwnerID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
