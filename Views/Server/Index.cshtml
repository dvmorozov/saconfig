@model IEnumerable<SAConfig.Models.saconfig_tServer>

@{
    ViewBag.Title = "Server";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Server</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { id = (long)ViewBag.UpperLevelID, backURL = (string)(ViewBag.BackURL) })
</p>
<table>
    <tr>
        <th>
            desc
        </th>
        <th>
            timeout
        </th>
        <th>
            AccessPoint
        </th>
        <th>
            DataOwnerID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.timeout)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tAccessPoint.desc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataOwnerID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.ID, upperLevelID = (long)ViewBag.UpperLevelID, backURL = (string)(ViewBag.BackURL) }) |
            @Html.ActionLink("Details", "Details", new { id = item.ID, upperLevelID = (long)ViewBag.UpperLevelID, backURL = (string)(ViewBag.BackURL) }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.ID, upperLevelID = (long)ViewBag.UpperLevelID, backURL = (string)(ViewBag.BackURL) })
        </td>
    </tr>
}

</table>
