@model SAConfig.Models.saconfig_tGSESettings

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Edit</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>GSESettings Attributes</legend>

        @Html.HiddenFor(model => model.ID)

        <div class="editor-label">
            @Html.LabelFor(model => model.appID, "appID")
        </div>
        <div class="editor-field">
            @Html.DropDownList("appID", String.Empty)
            @Html.ValidationMessageFor(model => model.appID)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.dataLabel, "dataLabel")
        </div>
        <div class="editor-field">
            @Html.DropDownList("dataLabel", String.Empty)
            @Html.ValidationMessageFor(model => model.dataLabel)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Services, "Services")
        </div>
        <div class="editor-field">
            @Html.DropDownList("Services", String.Empty)
            @Html.ValidationMessageFor(model => model.Services)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
