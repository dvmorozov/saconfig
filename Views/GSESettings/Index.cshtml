@model IEnumerable<SAConfig.Models.saconfig_tGSESettings>

@{
    ViewBag.Title = "GSESettings";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GSESettings</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            appID
        </th>
        <th>
            dataLabel
        </th>
        <th>
            Services
        </th>
        <th>
            DataOwnerID
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tServiceSettingsEnum.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tServiceSettingsEnum1.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tServices.ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataOwnerID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
