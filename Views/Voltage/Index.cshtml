@model IEnumerable<SAConfig.Models.saconfig_tVoltage>

@{
    ViewBag.Title = "Voltage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Voltage</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            DataOwnerID
        </th>
        <th>
            unit
        </th>
        <th>
            multiplier
        </th>
        <th>
            VoltageLevel
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DataOwnerID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tSIUnitEnum.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tUnitMultiplierEnum.value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.saconfig_tVoltageLevel.desc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
